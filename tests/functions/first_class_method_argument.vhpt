--TEST--
First-class callable from method passed as function argument
--FILE--
<?php
class Greeter {
    public function greet($name) {
        return "Hello, " . $name . "!";
    }
}

function apply_callback($value, $callback) {
    return $callback($value);
}

$greeter = new Greeter();
echo apply_callback("World", $greeter->greet(...));
--EXPECT--
Hello, World!
