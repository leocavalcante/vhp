--TEST--
Generator::send() and Generator::throw() method stubs
--FILE--
<?php
function gen() {
    yield 1;
    yield 2;
    yield 3;
}

$g = gen();
$g->next();
echo "Current: " . $g->current() . "\n";

// Test send() - stub behavior advances and returns next yielded value
$result = $g->send("test");
echo "Send returned: " . (is_int($result) ? $result : "not int") . "\n";
?>
--EXPECT--
Current: 2
Send returned: 3

