--TEST--
Anonymous class with constructor arguments
--FILE--
<?php
$obj = new class("World") {
    private $name;
    
    public function __construct($name) {
        $this->name = $name;
    }
    
    public function greet() {
        return "Hello, " . $this->name;
    }
};

echo $obj->greet();
--EXPECT--
Hello, World
