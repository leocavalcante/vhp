--TEST--
Clone with - property values can be expressions
--FILE--
<?php
class Point {
    public function __construct(
        public float $x,
        public float $y
    ) {}
}

$p1 = new Point(1.0, 2.0);
$factor = 2.0;
$p2 = clone $p1 with { x: $p1->x * $factor, y: $p1->y + 10.0 };

echo "p2: " . $p2->x . ", " . $p2->y;
--EXPECT--
p2: 2, 12
