--TEST--
Attribute Reflection - attribute with positional arguments
--FILE--
<?php
#[Route("/api/users", "GET")]
class UserController {}

$attrs = get_class_attributes("UserController");
$args = $attrs[0]["arguments"];
echo count($args);
echo "\n";
echo $args[0]["value"];
echo "\n";
echo $args[1]["value"];
--EXPECT--
2
/api/users
GET
