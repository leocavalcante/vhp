--TEST--
Attributes - attribute on trait
--FILE--
<?php
#[Timestampable]
trait Timestamps {
    public $created_at;
    public $updated_at;
}

class Post {
    use Timestamps;

    public function __construct() {
        $this->created_at = "2023-01-01";
        echo $this->created_at;
    }
}

$post = new Post();
--EXPECT--
2023-01-01
